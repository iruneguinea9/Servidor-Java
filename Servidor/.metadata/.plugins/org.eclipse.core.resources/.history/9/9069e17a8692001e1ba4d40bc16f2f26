package beans;

import java.util.ArrayList;

public class AlmacenPalabras {

	private static ArrayList<String> listaPalabras=new ArrayList<String>();
	
	public static String palabraAzar() {
		String palabra="";
		int indice = (int) (Math.random()*listaPalabras.size()+1);
		
		
		return palabra;
	}
}
